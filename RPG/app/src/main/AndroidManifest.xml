<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.saurabh.rpg" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GenerateActivity"
            android:label="@string/title_activity_generate" >
        </activity>
        <activity
            android:name=".NewPasswordActivity"
            android:label="@string/title_activity_new_password" >
        </activity>
        <activity
            android:name=".PasswordListActivity"
            android:label="@string/title_activity_password_list" >
        </activity>
    </application>

</manifest>
